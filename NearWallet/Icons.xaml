    <Style x:Key="ButtonRectangle" TargetType="Rectangle">
        <Setter Property="Width" Value="25" />
        <Setter Property="Height" Value="25" />

        <Setter Property="OpacityMask" Value="{StaticResource walletIcon}" />

        <Setter Property="Fill" Value="Black" />
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=IsPressed, RelativeSource={RelativeSource AncestorType=Button}}" Value="True">
                <Setter Property="Fill" Value="Blue" />
            </DataTrigger>
        </Style.Triggers>
    </Style>



    <Style x:Key="rectIcon" TargetType="Rectangle">
        <Setter Property="Width" Value="25" />
        <Setter Property="Height" Value="25" />
        <Setter Property="Fill" Value="Gray" />
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=IsSelected, RelativeSource={RelativeSource AncestorType=TabItem}}" Value="True">
                <Setter Property="Fill" Value="#FF4B7FBD" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IsMouseOver, RelativeSource={RelativeSource AncestorType=TabItem}}" Value="True">
                <Setter Property="Fill" Value="#FF4B7FBD" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IsMouseOver, RelativeSource={RelativeSource AncestorType=Button}}" Value="True">
                <Setter Property="Fill" Value="#FF4B7FBD" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IsMouseOver, RelativeSource={RelativeSource AncestorType=ToggleButton}}" Value="True">
                <Setter Property="Fill" Value="#FF4B7FBD" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IsChecked, RelativeSource={RelativeSource AncestorType=ToggleButton}}" Value="True">
                <Setter Property="Fill" Value="#FF4B7FBD" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="rectWalletIcon" TargetType="Rectangle" BasedOn="{StaticResource rectIcon}">
        <Setter Property="OpacityMask" Value="{StaticResource walletIcon}" />
    </Style>

    <Style x:Key="rectAccountIcon" TargetType="Rectangle" BasedOn="{StaticResource rectIcon}">
        <Setter Property="OpacityMask" Value="{StaticResource accountIcon}"  />
